?-nearby(W, charing_cross)
			nearby(X,Y) :- connected(X, Y, L).
	{X -> W, Y -> charing_cross}
?-connected(W, charing_cross, L)
			connected(green_park, charing_cross, jubilee)
	{W -> green_park, L -> jubilee}
[]



